<Page class="page"
    navigatingTo="onNavigatingTo"
    xmlns="http://schemas.nativescript.org/tns.xsd">

    <ActionBar class="action-bar">
        <Label class="action-bar-title" text="JS Benchmarks"></Label>
    </ActionBar>
    <ScrollView>
      <StackLayout>
        <Label class="title" text="{{ score }}"></Label>
        <Label class="label" text="{{ messages }}" textWrap="true"></Label>
      </StackLayout>
    </ScrollView>
</Page>
